<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feed.projecctfeed">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_fb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_fb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Views.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BaseController.BaseActivity" />
        <activity android:name=".Views.FeedScreen"/>
        <activity android:name=".Views.FeedDetails"/>
    </application>

</manifest>